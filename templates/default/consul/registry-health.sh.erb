#!/usr/bin/env bash

CHECK="curl --fail --silent --insecure --key <%= node["kagent"]["certs_dir"] %>/priv.key --cert <%= node["kagent"]["certs"]["elastic_host_certificate"] %> https://$HOSTNAME:<%= node['hops']['docker']['registry']['port'] %> > /dev/null 2>&1"

source <%= node['consul']['health-check']['retryable-check-file'] %>

_run_health_check "$CHECK"
